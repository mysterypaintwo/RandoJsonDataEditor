{
    "version": "0.2.0",
    "configurations": [
        {
        "type": "node",
        "request": "launch",
        "name": "Electron: Main",
        "runtimeExecutable": "node",
        "runtimeArgs": [
            "${workspaceFolder}/node_modules/electron/cli.js",
            "--remote-debugging-port=9223",
            "."
        ],
        "windows": {
            "runtimeExecutable": "node"
        }
        },
        {
            "name": "Electron: Renderer",
            "type": "chrome",
            "request": "attach",
            "port": 9223,
            "webRoot": "${workspaceFolder}",
            "timeout": 30000
        }
    ],
    "compounds": [
        {
            "name": "Electron: All",
            "configurations": [
                "Electron: Main",
                "Electron: Renderer"
            ]
        }
    ]
  }